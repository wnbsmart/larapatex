{
    "name": "wnbsmart/larapatex",
    "description": "c-patex.com API wrapper for Laravel",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "c-patex",
        "cryptocurrency",
        "exchange"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "wnbsmart",
            "email": "maroje.macola@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "ext-curl": "*",
        "laravel/framework": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "wnbsmart\\larapatex\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wnbsmart\\Larapatex\\CpatexServiceProvider"
            ],
            "aliases": {
                "Cpatex": "Wnbsmart\\Larapatex\\CpatexAPIFacade"
            }
        }
    }

}
